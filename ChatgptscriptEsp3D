local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/ftgs/WindUI/main/library.lua"))()

-- ‚ë° ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏´‡∏ô‡πâ‡∏≤‡∏ï‡πà‡∏≤‡∏á‡∏´‡∏•‡∏±‡∏Å
local Window = WindUI:CreateWindow({
    Title = "My Hub",
    SubTitle = "by goskdeg",
    Theme = "Dark"
})

-- ‚ë¢ ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÅ‡∏ó‡πá‡∏ö‡πÉ‡∏ô‡∏´‡∏ô‡πâ‡∏≤‡∏ï‡πà‡∏≤‡∏á
local Tab = Window:Tab({
    Title = "Main",
    Icon = "user"
})

------------------------------------------------------------------
-- üîπ ‡∏™‡πà‡∏ß‡∏ô‡∏ô‡∏µ‡πâ‡∏Ñ‡∏∑‡∏≠‡∏ó‡∏µ‡πà‡πÑ‡∏ß‡πâ "‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì" ‡πÄ‡∏ä‡πà‡∏ô ESP, AutoFarm, ‡∏Ø‡∏•‡∏Ø
------------------------------------------------------------------

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á Highlight ‡∏£‡∏≠‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
local function createESP(player)
    if player.Character and not player.Character:FindFirstChild("ESPHighlight") then
        local highlight = Instance.new("Highlight")
        highlight.Name = "ESPHighlight"
        highlight.FillColor = Color3.fromRGB(255, 128, 0)
        highlight.OutlineColor = Color3.fromRGB(255, 128, 0)
        highlight.FillTransparency = 0.5
        highlight.Parent = player.Character
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏•‡∏ö Highlight
local function removeESP(player)
    if player.Character and player.Character:FindFirstChild("ESPHighlight") then
        player.Character.ESPHighlight:Destroy()
    end
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ESP ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
local function toggleESP(state)
    local players = game:GetService("Players")
    local localPlayer = players.LocalPlayer

    if state then
        print("ESP ‡πÄ‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡πâ‡∏ß ‚úÖ")
        for _, player in pairs(players:GetPlayers()) do
            if player ~= localPlayer then
                createESP(player)
            end
        end

        players.PlayerAdded:Connect(function(player)
            player.CharacterAdded:Connect(function()
                wait(1)
                createESP(player)
            end)
        end)
    else
        print("ESP ‡∏õ‡∏¥‡∏î‡πÅ‡∏•‡πâ‡∏ß ‚ùå")
        for _, player in pairs(players:GetPlayers()) do
            removeESP(player)
        end
    end
end

------------------------------------------------------------------
-- üî∏ ‡∏™‡πà‡∏ß‡∏ô‡∏ô‡∏µ‡πâ‡∏Ñ‡∏∑‡∏≠ "UI Elements" ‡πÄ‡∏ä‡πà‡∏ô Toggle, Button, Input ‡∏Ø‡∏•‡∏Ø
------------------------------------------------------------------

-- Toggle ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ESP
local Toggle = Tab:Toggle({
    Title = "ESP Toggle",
    Desc = "‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î ‡∏Å‡∏≤‡∏£‡∏°‡∏≠‡∏á‡πÄ‡∏´‡πá‡∏ô‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô",
    Icon = "eye",
    Type = "Checkbox",
    Value = false,
    Callback = function(state)
        toggleESP(state)  -- üëà ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡πÉ‡∏ä‡πâ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏£‡∏≤‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏î‡πâ‡∏≤‡∏ô‡∏ö‡∏ô
    end
})

------------------------------------------------------------------
-- üî∏ ‡∏à‡∏∞‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏õ‡∏∏‡πà‡∏°‡∏≠‡∏∑‡πà‡∏ô‡πÜ ‡∏ï‡πà‡∏≠‡πÑ‡∏î‡πâ‡∏ï‡∏£‡∏á‡∏ô‡∏µ‡πâ ‡πÄ‡∏ä‡πà‡∏ô:
------------------------------------------------------------------

local Button = Tab:Button({
    Title = "Test Button",
    Desc = "‡∏Å‡∏î‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ó‡∏î‡∏™‡∏≠‡∏ö",
    Icon = "zap",
    Callback = function()
        print("‡∏õ‡∏∏‡πà‡∏°‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß ‚ö°")
    end end
})
