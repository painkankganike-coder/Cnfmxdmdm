--------------------------------------------------------
-- ðŸŸ¢ Character Tab (WalkSpeed + Jump + Mud Warp)
--------------------------------------------------------
local Tab_Character = Window:Tab({
    Title = "Character",
    Icon = "user",
})

-- WalkSpeed
local SpeedEnabled = false
local DefaultSpeed = 16
local CustomSpeed = 50

Tab_Character:Toggle({
    Title = "Walk Speed",
    Desc = "à¹€à¸›à¸´à¸”/à¸›à¸´à¸”à¸„à¸§à¸²à¸¡à¹€à¸£à¹‡à¸§à¹€à¸”à¸´à¸™",
    Icon = "zap",
    Value = false,
    Callback = function(state)
        SpeedEnabled = state
        local humanoid = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
        if humanoid then
            if state then
                humanoid.WalkSpeed = CustomSpeed
                print("âœ… à¹€à¸›à¸´à¸”à¹‚à¸«à¸¡à¸” Speed (" .. CustomSpeed .. ")")
            else
                humanoid.WalkSpeed = DefaultSpeed
                print("âŒ à¸›à¸´à¸”à¹‚à¸«à¸¡à¸” Speed à¸à¸¥à¸±à¸šà¹€à¸›à¹‡à¸™à¸›à¸à¸•à¸´")
                humanoid.WalkSpeed = DefaultSpeed
            end
        end
    end
})

Tab_Character:Slider({
    Title = "Speed Value",
    Desc = "à¸›à¸£à¸±à¸šà¸£à¸°à¸”à¸±à¸šà¸„à¸§à¸²à¸¡à¹€à¸£à¹‡à¸§à¸à¸²à¸£à¹€à¸”à¸´à¸™",
    Step = 1,
    Value = {Min = 16, Max = 50, Default = 16},
    Callback = function(value)
        CustomSpeed = value
        if SpeedEnabled then
            local humanoid = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = value
            end
        end
    end
})

--------------------------------------------------------
-- ðŸŸ¢ Super Jump
--------------------------------------------------------
local JumpEnabled = false
local JumpPower = 100 -- à¸•à¸±à¹‰à¸‡à¸„à¹ˆà¸²à¸à¸£à¸°à¹‚à¸”à¸”

Tab_Character:Toggle({
    Title = "Super Jump",
    Desc = "à¹€à¸›à¸´à¸”/à¸›à¸´à¸”à¸à¸£à¸°à¹‚à¸”à¸”à¸ªà¸¹à¸‡",
    Icon = "arrow-up",
    Value = false,
    Callback = function(state)
        JumpEnabled = state
        local humanoid = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
        if humanoid then
            if state then
                humanoid.JumpPower = JumpPower
            else
                humanoid.JumpPower = 50
            end
        end
    end
})

--------------------------------------------------------
-- ðŸŸ¢ Mud Warp (à¸¡à¸¸à¸”à¸”à¸´à¸™)
--------------------------------------------------------
local MudEnabled = false
local MudDepth = 10 -- à¸„à¸§à¸²à¸¡à¸¥à¸¶à¸à¹€à¸£à¸´à¹ˆà¸¡à¸•à¹‰à¸™

Tab_Character:Toggle({
    Title = "Snap",
    Desc = "à¹€à¸›à¸´à¸”/à¸›à¸´à¸” à¸¡à¸¸à¸”à¸”à¸´à¸™",
    Icon = "arrow-down",
    Value = false,
    Callback = function(state)
        MudEnabled = state
        local char = game.Players.LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            local hrp = char.HumanoidRootPart
            if state then
                hrp.CFrame = hrp.CFrame - Vector3.new(0, MudDepth, 0)
                print("âœ… Mud Warp ON | Depth: " .. MudDepth)
            else
                hrp.CFrame = hrp.CFrame + Vector3.new(0, MudDepth, 0)
                print("âŒ Mud Warp OFF")
            end
        end
    end
})

Tab_Character:Slider({
    Title = "Snap",
    Desc = "à¸›à¸£à¸±à¸šà¸„à¸§à¸²à¸¡à¸¥à¸¶à¸à¹ƒà¸™à¸à¸²à¸£à¸¡à¸¸à¸”à¸”à¸´à¸™",
    Step = 1,
    Value = {Min = 1, Max = 50, Default = MudDepth},
    Callback = function(value)
        MudDepth = value
        if MudEnabled then
            local char = game.Players.LocalPlayer.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                local hrp = char.HumanoidRootPart
                hrp.CFrame = hrp.CFrame - Vector3.new(0, MudDepth, 0)
            end
        end
    end
})
