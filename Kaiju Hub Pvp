    Icon = "circle-check",
    Default = true,
    Callback = function(state)
        getgenv().DroppedItemESPEnabled = state

        -- à¸–à¹‰à¸²à¸›à¸´à¸” -> à¸‹à¹ˆà¸­à¸™ ESP à¸—à¸±à¹‰à¸‡à¸«à¸¡à¸”à¸—à¸±à¸™à¸—à¸µ
