local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

local PingGui = Instance.new("ScreenGui")
PingGui.Name = "PingDisplay"
PingGui.ResetOnSpawn = false
PingGui.Parent = PlayerGui
PingGui.DisplayOrder = 999
PingGui.Enabled = false -- à¹€à¸£à¸´à¹ˆà¸¡à¸›à¸´à¸”

local pingLabel = Instance.new("TextLabel")
pingLabel.Size = UDim2.new(0, 120, 0, 30)
pingLabel.Position = UDim2.new(0, 10, 0, 10) -- à¸¡à¸¸à¸¡à¸‹à¹‰à¸²à¸¢à¸šà¸™
pingLabel.BackgroundTransparency = 1
pingLabel.Font = Enum.Font.SourceSansBold
pingLabel.TextSize = 18
pingLabel.TextXAlignment = Enum.TextXAlignment.Left
pingLabel.TextYAlignment = Enum.TextYAlignment.Top
pingLabel.Text = "Ping: 0 ms"
pingLabel.Parent = PingGui

local pingStroke = Instance.new("UIStroke")
pingStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Contextual
pingStroke.Color = Color3.fromRGB(0,0,0)
pingStroke.Thickness = 2
pingStroke.Parent = pingLabel

local ShowPing = false

local FpsGui = Instance.new("ScreenGui")
FpsGui.Name = "FPSDisplay"
FpsGui.ResetOnSpawn = false
FpsGui.Parent = PlayerGui
FpsGui.DisplayOrder = 999
FpsGui.Enabled = false -- à¹€à¸£à¸´à¹ˆà¸¡à¸›à¸´à¸”

local fpsLabel = Instance.new("TextLabel")
fpsLabel.Size = UDim2.new(0, 120, 0, 30)
fpsLabel.Position = UDim2.new(0, 10, 0, 25) -- à¸­à¸¢à¸¹à¹ˆà¹ƒà¸•à¹‰ Ping
fpsLabel.BackgroundTransparency = 1
fpsLabel.Font = Enum.Font.SourceSansBold
fpsLabel.TextSize = 18
fpsLabel.TextXAlignment = Enum.TextXAlignment.Left
fpsLabel.TextYAlignment = Enum.TextYAlignment.Top
fpsLabel.Text = "FPS: 0"
fpsLabel.Parent = FpsGui

local fpsStroke = Instance.new("UIStroke")
fpsStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Contextual
fpsStroke.Color = Color3.fromRGB(0,0,0)
fpsStroke.Thickness = 2
fpsStroke.Parent = fpsLabel

local ShowFPS = false
local lastTime = tick()
local frameCount = 0

RunService.RenderStepped:Connect(function()
    -- Ping
    if ShowPing then
        local ping = math.floor(LocalPlayer:GetNetworkPing() * 1000)
        pingLabel.Text = "Ping: "..ping.." ms"
        if ping > 100 then
            pingLabel.TextColor3 = Color3.fromRGB(255,0,0) -- à¹à¸”à¸‡
        elseif ping >= 70 and ping <= 80 then
            pingLabel.TextColor3 = Color3.fromRGB(255,255,0) -- à¹€à¸«à¸¥à¸·à¸­à¸‡
        elseif ping < 60 then
            pingLabel.TextColor3 = Color3.fromRGB(0,255,0) -- à¹€à¸‚à¸µà¸¢à¸§
        else
            pingLabel.TextColor3 = Color3.fromRGB(255,165,0) -- à¸ªà¹‰à¸¡
        end
    end

    -- FPS
    if ShowFPS then
        frameCount = frameCount + 1
        local currentTime = tick()
        if currentTime - lastTime >= 1 then
            fpsLabel.Text = "FPS: " .. frameCount
            -- à¹€à¸›à¸¥à¸µà¹ˆà¸¢à¸™à¸ªà¸µ FPS
            if frameCount < 30 then
                fpsLabel.TextColor3 = Color3.fromRGB(255,0,0) -- à¹à¸”à¸‡
            elseif frameCount >= 30 and frameCount <= 60 then
                fpsLabel.TextColor3 = Color3.fromRGB(255,255,0) -- à¹€à¸«à¸¥à¸·à¸­à¸‡
            else
                fpsLabel.TextColor3 = Color3.fromRGB(0,255,0) -- à¹€à¸‚à¸µà¸¢à¸§
            end
            frameCount = 0
            lastTime = currentTime
        end
    end
end)



