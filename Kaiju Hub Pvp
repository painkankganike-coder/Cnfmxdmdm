    Title = "Kaiju Hub | Pvp Paid ðŸ’µ",
    Icon = "gitlab",
    Author = "My Kaiju | BlockSpinðŸ”«",
    Folder = "XINCOHubFolder",
    Size = UDim2.fromOffset(400, 350),
    Theme = "Dark",
    Transparent = true,
    Resizable = true,
})

Window:Tag({
    Title = "v0.6.8",
    Icon = "cloud",
