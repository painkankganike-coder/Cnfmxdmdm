CombatTab:Input({
    Title = "Safe Players",
    Placeholder = "‡πÉ‡∏™‡πà‡∏ä‡∏∑‡πà‡∏≠‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡πÑ‡∏°‡πà‡∏•‡πá‡∏≠‡∏Ñ, ‡∏Ñ‡∏±‡πà‡∏ô‡∏î‡πâ‡∏ß‡∏¢ ,",
    InputIcon = "shield-check",
    Callback = function(txt)
        IgnorePlayers = {}
        for inputName in string.gmatch(txt, '([^,]+)') do
            inputName = inputName:match("^%s*(.-)%s*$")
            table.insert(IgnorePlayers, inputName)
        end
    end
})

task.spawn(function()
    while true do
        task.wait(60)
        for i=#IgnorePlayers,1,-1 do
            local name = IgnorePlayers[i]
            local exists = false
            for _, plr in ipairs(Players:GetPlayers()) do
                if string.lower(plr.Name) == string.lower(name) then
                    exists = true
                    break
                end
            end
            if not exists then
                table.remove(IgnorePlayers, i)
            end
        end
    end
end)

CombatTab:Divider()

local ReloadMultiplierValue = Instance.new("NumberValue")
ReloadMultiplierValue.Name = "ReloadMultiplier"
ReloadMultiplierValue.Value = 1.0
ReloadMultiplierValue.Parent = LocalPlayer

WeaponTab:Slider({
    Title = "Reload time",
    Step = 0.5,
    Value = {Min = 0.0, Max = 1.5, Default = ReloadMultiplierValue.Value},
    Callback = function(value)
        ReloadMultiplierValue.Value = value
    end
})

task.spawn(function()
    while true do
        for _, v in pairs(getgc(true)) do
            if typeof(v) == "table" and rawget(v, "states") then
                local s = v.states

                -- Reload Speed
                if s.reload_time and s.reload_time.set then
                    local original = s.reload_time:get()
                    if original > 0 then
                        s.reload_time:set(original / ReloadMultiplierValue.Value)
                    end
                end
            end
        end
        task.wait(0.3)
    end
end)


